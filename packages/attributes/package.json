{
  "name": "@finsweet/attributes",
  "version": "2.4.1",
  "description": "Finsweet Attributes V2.",
  "homepage": "https://finsweet.com/attributes",
  "type": "module",
  "main": "attributes.js",
  "files": [
    "attributes.js",
    "dist",
    "schemas"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx ./bin/build.ts",
    "build": "cross-env NODE_ENV=production tsx ./bin/build.ts",
    "lint": "eslint ./src && prettier --check ./src",
    "lint:fix": "eslint ./src --fix && prettier --write ./src",
    "check": "tsc --noEmit",
    "test": "pnpm playwright test",
    "test:headed": "pnpm playwright test --headed"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/attributes.git"
  },
  "keywords": [
    "webflow",
    "attributes",
    "no code"
  ],
  "author": {
    "name": "Finsweet",
    "url": "https://finsweet.com/"
  },
  "bugs": {
    "url": "https://finsweet.com/attributes/support"
  },
  "dependencies": {
    "@finsweet/attributes-accordion": "workspace:*",
    "@finsweet/attributes-autovideo": "workspace:*",
    "@finsweet/attributes-codehighlight": "workspace:*",
    "@finsweet/attributes-combobox": "workspace:*",
    "@finsweet/attributes-copyclip": "workspace:*",
    "@finsweet/attributes-displayvalues": "workspace:*",
    "@finsweet/attributes-favcustom": "workspace:*",
    "@finsweet/attributes-formsubmit": "workspace:*",
    "@finsweet/attributes-inject": "workspace:*",
    "@finsweet/attributes-inputactive": "workspace:*",
    "@finsweet/attributes-inputcounter": "workspace:*",
    "@finsweet/attributes-list": "workspace:*",
    "@finsweet/attributes-mirrorclick": "workspace:*",
    "@finsweet/attributes-mirrorinput": "workspace:*",
    "@finsweet/attributes-modal": "workspace:*",
    "@finsweet/attributes-numbercount": "workspace:*",
    "@finsweet/attributes-queryparam": "workspace:*",
    "@finsweet/attributes-rangeslider": "workspace:*",
    "@finsweet/attributes-readtime": "workspace:*",
    "@finsweet/attributes-removequery": "workspace:*",
    "@finsweet/attributes-scrolldisable": "workspace:*",
    "@finsweet/attributes-selectcustom": "workspace:*",
    "@finsweet/attributes-sliderdots": "workspace:*",
    "@finsweet/attributes-smartlightbox": "workspace:*",
    "@finsweet/attributes-socialshare": "workspace:*",
    "@finsweet/attributes-starrating": "workspace:*",
    "@finsweet/attributes-toc": "workspace:*",
    "@finsweet/attributes-utils": "workspace:*",
    "@finsweet/attributes-videohls": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.15.12",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.4",
    "esbuild-plugin-inline-worker": "^0.1.1",
    "tsx": "^4.19.4"
  }
}
