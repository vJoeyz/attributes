{
  "name": "@finsweet/attributes",
  "description": "Finsweet's Attributes monorepo.",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/attributes.git"
  },
  "author": {
    "name": "Finsweet",
    "url": "https://www.finsweet.com/"
  },
  "bugs": {
    "url": "https://github.com/finsweet/attributes/issues"
  },
  "homepage": "https://github.com/finsweet/attributes#readme",
  "scripts": {
    "build": "pnpm -r --filter \"./packages/**\" build",
    "build:dev": "pnpm -r --filter \"./packages/**\" build:dev",
    "lint": "pnpm -r --filter \"./packages/**\" lint",
    "lint:fix": "pnpm -r --filter \"./packages/**\" lint:fix",
    "check": "pnpm -r --filter \"./packages/**\" check",
    "format": "pnpm -r --filter \"./packages/**\" format",
    "test": "pnpm -r --filter \"./packages/**\" --filter !@finsweet/attributes-support test --workspace-concurrency=1",
    "test:headed": "pnpm -r --filter \"./packages/**\" --filter !@finsweet/attributes-support test:headed --workspace-concurrency=1",
    "release": "changeset publish",
    "update": "pnpm update -i -L -r"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.1.14",
    "@changesets/cli": "^2.26.1",
    "@finsweet/eslint-config": "^2.0.4",
    "@finsweet/tsconfig": "^1.3.1",
    "@playwright/test": "^1.32.3",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.16",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@finsweet/ts-utils": "^0.39.1",
    "nanoid": "^4.0.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "body-scroll-lock@4.0.0-beta.0": "patches/body-scroll-lock@4.0.0-beta.0.patch"
    }
  }
}
